{
  "name": "compositex",
  "description": "composite nodes, run pipeline",
  "version": "1.0",
  "manifest_version": 3,
  "icons": {
    "48": "images/48.png"
  },
  "action": {
    "default_popup": "popup.html"
  },
  "options_page": "options.html",
  "permissions": ["tabs"],
  "host_permissions": ["<all_urls>"],
  "content_security_policy": {
    "extension_pages": "script-src 'self' 'unsafe-eval'; object-src 'self'"
  }
}
